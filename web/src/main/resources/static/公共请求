//下拉框模板 展现形式   例如：输入框，选择框
showGrid({
 	id: "showModel",
 	url: flowableBasePath + "dsform006",
 	params: {},
 	pagination: false,
 	template: getFileContent('tpl/template/select-option.tpl'),
 	ajaxSendLoadBefore: function(hdb){
 	},
 	ajaxSendAfter:function (json) {
 		form.render('select');
 	}
});

